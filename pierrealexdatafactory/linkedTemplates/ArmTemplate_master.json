{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pierrealexdatafactory"
		},
		"AzureBlobStoragepierrealex_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStoragepierrealex'"
		},
		"AzureDataLakeStorageflights_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageflights'"
		},
		"AzureDataLakeStorageflights2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageflights2'"
		},
		"AzureDataLakeStorageflights_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azpierrealexstorage.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageflights2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdatalakepierrealex.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pierrealexdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStoragepierrealex_connectionString": {
						"value": "[parameters('AzureBlobStoragepierrealex_connectionString')]"
					},
					"AzureDataLakeStorageflights_accountKey": {
						"value": "[parameters('AzureDataLakeStorageflights_accountKey')]"
					},
					"AzureDataLakeStorageflights2_accountKey": {
						"value": "[parameters('AzureDataLakeStorageflights2_accountKey')]"
					},
					"AzureDataLakeStorageflights_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageflights_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageflights2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageflights2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}